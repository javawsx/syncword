<!doctype html>
<html lang="ch" data-layout="vertical" data-topbar="light" data-sidebar="dark" data-sidebar-size="lg"
  data-sidebar-image="none" data-preloader="disable">

<head>
  <meta charset="utf-8" />
  <title>
    <%-title%> | 2CY.GAME - Pack Template
  </title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta content="Premium Multipurpose Admin & Dashboard Template" name="description" />
  <meta content="Themesbrand" name="author" />
  <!-- App favicon -->
  <link rel="shortcut icon" href="assets/images/favicon.ico">
  <%- include('layout/head-css'); %>
    <!--datatable css-->
    <link rel="stylesheet" href="https://cdn.datatables.net/1.11.5/css/dataTables.bootstrap5.min.css" />
    <!--datatable responsive css-->
    <link rel="stylesheet" href="https://cdn.datatables.net/responsive/2.2.9/css/responsive.bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/2.2.2/css/buttons.dataTables.min.css">
    <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />
    <link href="assets/libs/sweetalert2/sweetalert2.min.css" rel="stylesheet" type="text/css" />
    <!-- dropzone css -->
    <link rel="stylesheet" href="assets/libs/dropzone/dropzone.css" type="text/css" />
</head>

<body>
  <div id="layout-wrapper">
    <%- include('layout/topbar'); %>
      <%- include('layout/sidebar'); %>
        <div class="main-content overflow-hidden">
          <div class="page-content">
            <div class="container-fluid">
              <%- include('layout/page-title'); %>
                <div class="row justify-content-center">
                  <div class="col-xxl-9">
                    <div class="card">
                      <form class="needs-validation" novalidate id="invoice_form">
                        <div class="card-body border-bottom border-bottom-dashed p-4">
                          <div class="row">
                            <div class="col-lg-4">
                              <div class="modal-body">
                                <div class="mb-3">
                                  <label for="GameId" class="form-label" data-key="t-game-id">
                                    GameId</label>
                                  <input type="number" id="GameId" class="form-control" required />
                                </div>
                                <div class="mb-3">
                                  <label for="GameName" class="form-label" data-key="t-game-name">
                                    GameName</label>
                                  <input type="text" id="GameName" class="form-control" required />
                                </div>

                                <div class="mb-3">
                                  <label for="Category" class="form-label" data-key="t-category">Category</label>
                                  <input type="number" id="Category" class="form-control" placeholder="1,2" required />
                                </div>

                                <div class="mb-3">
                                  <label for="Type" class="form-label" data-key="t-type">Type</label>
                                  <input type="text" id="Type" class="form-control" placeholder="RPG" required />
                                </div>

                                <div class="mb-3">
                                  <label for="Price" class="form-label" data-key="t-price">Price</label>
                                  <input type="number" id="Price" class="form-control" placeholder="0.00" required />
                                </div>

                                <div class="mb-3">
                                  <label for="Uri" class="form-label" data-key="t-uri">Uri</label>
                                  <input type="text" id="Uri" class="form-control" required />
                                </div>
                                <div class="mb-3">
                                  <label for="Status" class="form-label" data-key="t-status">Status</label>
                                  <input type="number" id="Status" class="form-control" placeholder="1,2" required />
                                </div>
                              </div>
                            </div>
                            <!--end col-->
                            <div class="col-lg-4 ms-auto">
                              <div class="modal-body">


                                <div class="mb-3">
                                  <label for="Publisher" class="form-label" data-key="t-publisher">Publisher</label>
                                  <input type="text" id="Publisher" class="form-control" required />
                                </div>

                                <div class="mb-3">
                                  <label for="Developer" class="form-label" data-key="t-developer">Developer</label>
                                  <input type="text" id="Developer" class="form-control" required />
                                </div>

                                <div class="mb-3">
                                  <label for="Brief" class="form-label" data-key="t-brief">Brief</label>
                                  <input type="text" id="Brief" class="form-control" required />
                                </div>

                                <div class="mb-3">
                                  <label for="Desc" class="form-label" data-key="t-desc">Desc</label>
                                  <input type="text" id="Desc" class="form-control" required />
                                </div>

                                <div class="mb-3">
                                  <label for="UrlPC" class="form-label" data-key="t-url-pc">UrlPC</label>
                                  <input type="text" id="UrlPC" class="form-control" required />
                                </div>

                                <div class="mb-3">
                                  <label for="UrlMobile" class="form-label" data-key="t-url-mobile">UrlMobile</label>
                                  <input type="text" id="UrlMobile" class="form-control" required />
                                </div>

                              </div>
                            </div>
                          </div>
                          <!--end row-->
                          <div class="hstack gap-2 justify-content-end d-print-none mt-4">
                            <button type="submit" class="btn btn-success" id="add-btn"><i
                                class="ri-add-line align-bottom me-1"></i>
                              Add</button>
                            <button type="reset" class="btn btn-danger"><i
                                class="ri-printer-line align-bottom me-1"></i>
                              Reset</button>
                          </div>
                        </div>
                      </form>
                    </div>
                  </div>
                  <!--end col-->
                </div>
                <!--end row-->


            </div>
            <!-- container-fluid -->
          </div>
          <!-- End Page-content -->
          <%- include('layout/footer'); %>

        </div>

        <!--end row-->
        <!-- end main content-->
  </div>
  <!-- END layout-wrapper -->
  <%- include('layout/customizer'); %>

    <script src="https://code.jquery.com/jquery-3.6.0.min.js"
      integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.5/js/dataTables.bootstrap5.min.js"></script>
    <script src="https://cdn.datatables.net/responsive/2.2.9/js/dataTables.responsive.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.2/js/dataTables.buttons.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.2/js/buttons.print.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.2/js/buttons.html5.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>
    <script src="assets/libs/sweetalert2/sweetalert2.min.js"></script>
    <script src="assets/libs/choices.js/public/assets/scripts/choices.min.js"></script>
    <script type="module" src="assets/js/game/game.add.js"></script>

    <%- include('layout/vendor-scripts'); %>
      <!-- App js -->
      <script src="assets/js/app.js"></script>
</body>

</html>